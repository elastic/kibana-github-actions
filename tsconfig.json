{
  "compilerOptions": {
    "target": "es2019",
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "lib": ["es2020"],
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true
  },
  "exclude": ["**/*.test.ts", "**/vm-filesystem/**"]
}
