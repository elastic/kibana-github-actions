    "@types/yargs": "^15.0.14",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chai": "^4.3.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "mocha": "^10.0.0",
    "nock": "^12.0.2",
    "prettier": "2.0.2",
    "typescript": "^4.7.3",
    "yargs": "^15.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add \"**/*.js\""
